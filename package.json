{
  "name": "@douglasduteil/crisp",
  "version": "1.2.0",
  "description": "âš¡ Typed Crisp API",
  "keywords": [
    "crisp",
    "integration",
    "wrapper",
    "api",
    "rest"
  ],
  "license": "WTFPL",
  "author": "Douglas Duteil <douglasduteil@gmail.com> (https://douglasduteil.github.io/)",
  "type": "module",
  "exports": {
    "./*": {
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "main": "./index.ts",
  "types": "./index.ts",
  "files": [
    "src",
    "index.ts"
  ],
  "release": {
    "npmPublish": false,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "npx prettier --write CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "tarballDir": ".semantic-release"
        }
      ],
      "@semantic-release/git",
      [
        "@semantic-release/github",
        {
          "assets": ".semantic-release/*.tgz"
        }
      ]
    ]
  },
  "devDependencies": {
    "@types/bun": "^1.1.4",
    "prettier": "^3.3.2",
    "typescript": "^5.4.5"
  }
}
